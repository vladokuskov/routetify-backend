{
  "name": "routetify",
  "version": "1.3",
  "description": "Web application that allows you to create routes.",
  "author": {
    "name": "Vladyslav Okuskov",
    "url": "https://github.com/vladokuskov"
  },
  "main": "app.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "start": "node build/src/server.js",
    "test": "jest --detectOpenHandles",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@mapbox/togeojson": "^0.16.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "http-status": "^1.6.2",
    "jest": "^29.7.0",
    "multer": "^1.4.5-lts.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "winston": "^3.10.0",
    "xmldom": "^0.6.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cookie": "^0.5.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.9",
    "@types/xmldom": "^0.1.32",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  }
}
